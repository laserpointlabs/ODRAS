  <script>
    $(function(){
        $(".home-content__title-important").typed({
          strings: [            "Innovative.",
                        "Bold.",
                        "Humble.",
                        "Grateful.",
                        "Courageous.",
                        ],
          typeSpeed: 100,
          loop: true,
          shuffle: true,
          backDelay: 2000
        });
    });
    $('.home-content__main').on('mouseenter', function(){
      $('.home-content').addClass('home-content--large-main');
    });
    $('.home-content__main').on('mouseleave', function(){
      $('.home-content').removeClass('home-content--large-main');
    });
    $('.home-content__sidebar').on('mouseenter', function(){
      $('.home-content').addClass('home-content--large-sidebar');
    });
    $('.home-content__sidebar').on('mouseleave', function(){
      $('.home-content').removeClass('home-content--large-sidebar');
    });

    $('.home-content__sidebar-section').on('mouseenter', function(){
      if($(this).hasClass('topSection')) {
        $('.home-content__sidebar').addClass('home-content__sidebar--large-top');
      } else {
        $('.home-content__sidebar').addClass('home-content__sidebar--large-bottom');
      }
    });
    $('.home-content__sidebar-section').on('mouseleave', function(){
      if($(this).hasClass('topSection')) {
        $('.home-content__sidebar').removeClass('home-content__sidebar--large-top');
      } else {
        $('.home-content__sidebar').removeClass('home-content__sidebar--large-bottom');
      }
    });


  </script>

