  "model": {
    "name": "BSEO_V2a",
    "type": "Model",
    "comment": "\"This ontology serves as the Base Systems Engineering Ontology, providing a structured framework for defining system requirements, components, functions, and processes, facilitating effective systems engineering practices.",
    "definition": "BSEO_V1 is an ontology designed to support the structured representation of system requirements and their relationships with components and processes, ensuring clarity and verifiability in systems engineering documentation.",
    "version": "",
    "namespace": "https://xma-adt.usnc.mil/odras/core/122847af-931e-4328-950f-c16d0d76b7bf/ontologies/a",
    "imports": ""
  },
  "nodes": [
    {
      "data": {
        "id": "Class1",
        "label": "Requirement",
        "type": "class",
        "attrs": {
          "comment": "Statement of need or obligation imposed on a system.",
          "definition": "A verifiable condition or capability the system shall satisfy.",
          "example": "“The UAV shall achieve ≥ 4 hr endurance at 10k ft.”",
          "creator": "jdehart",
          "created_date": "2025-10-07",
          "created_timestamp": "2025-10-07T10:17:53.921Z",
          "last_modified_by": "jdehart",
          "last_modified_date": "2025-10-15",
          "last_modified_timestamp": "2025-10-15T21:43:39.194Z"
        }
      },
      "position": {
        "x": -120,
        "y": 580
      }
    },
    {
      "data": {
        "id": "Class2",
        "label": "Component",
        "type": "class",
        "attrs": {
          "comment": "Physical or logical part of the system with defined interfaces.",
          "definition": "A replaceable system element that provides or supports functions.",
          "example": "“EO/IR sensor module,” “Wing spar,” “Flight-control app.”",
          "creator": "jdehart",
          "created_date": "2025-10-07",
          "created_timestamp": "2025-10-07T10:17:56.465Z",
          "last_modified_by": "jdehart",
          "last_modified_date": "2025-10-15",
          "last_modified_timestamp": "2025-10-15T21:43:39.195Z"
        }
      },
      "position": {
        "x": -120,
        "y": 760
      }
    },
    {
      "data": {
        "id": "Class3",
        "label": "Constraint",
        "type": "class",
        "attrs": {
          "comment": "Limitation that restricts design or operation.",
          "definition": "A binding limit (physical, legal, budgetary, schedule) on system states or choices.",
          "example": "“Max takeoff weight ≤ 25 kg due to Part 107.”",
          "creator": "jdehart",
          "created_date": "2025-10-07",
          "created_timestamp": "2025-10-07T10:18:00.611Z",
          "last_modified_by": "jdehart",
          "last_modified_date": "2025-10-15",
          "last_modified_timestamp": "2025-10-15T21:43:39.195Z"
        }
      },
      "position": {
        "x": 200,
        "y": 580
      }
    },
    {
      "data": {
        "id": "Class4",
        "label": "Interface",
        "type": "class",
        "attrs": {
          "comment": "Defined boundary where Components interact.",
          "definition": "A specification of interactions (signals, fluids, forces, data) between Components.",
          "example": "“Ethernet UDP telemetry @ 10 Hz between Autopilot and Mission PC.”",
          "creator": "jdehart",
          "created_date": "2025-10-07",
          "created_timestamp": "2025-10-07T10:18:04.485Z",
          "last_modified_by": "jdehart",
          "last_modified_date": "2025-10-15",
          "last_modified_timestamp": "2025-10-15T21:43:39.195Z"
        }
      },
      "position": {
        "x": 200,
        "y": 760
      }
    },
    {
      "data": {
        "id": "Class5",
        "label": "Function",
        "type": "class",
        "attrs": {
          "comment": "Intended capability realized by Processes.",
          "definition": "An abstract capability independent of specific implementation.",
          "example": "“Stabilize attitude,” “Detect targets,” “Provide ISR.”",
          "creator": "jdehart",
          "created_date": "2025-10-07",
          "created_timestamp": "2025-10-07T10:18:08.541Z",
          "last_modified_by": "jdehart",
          "last_modified_date": "2025-10-15",
          "last_modified_timestamp": "2025-10-15T21:43:39.195Z"
        }
      },
      "position": {
        "x": -260,
        "y": 980
      }
    },
    {
      "data": {
        "id": "Class6",
        "label": "Process",
        "type": "class",
        "attrs": {
          "comment": "Activity performed by Components to produce effects.",
          "definition": "An execution step transforming inputs into outputs under defined conditions.",
          "example": "“Assemble wing,” “Process video frames,” “Encrypt telemetry.”",
          "creator": "jdehart",
          "created_date": "2025-10-07",
          "created_timestamp": "2025-10-07T10:18:11.038Z",
          "last_modified_by": "jdehart",
